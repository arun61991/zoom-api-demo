<div class="column is-half ">  
  <% @users_data["users"].each do |user| %>
    <div class="box">
      <article class="media">
        <div class="media-left">
          <figure class="image is-64x64">
            <% if user['pic_url'].nil? %>
              <img src="https://bulma.io/images/placeholders/64x64.png">
            <% else %>
              <img src="<%=user['pic_url']%>">
            <% end %>
          </figure>
        </div>
        <div class="media-content">
          <div class="content">
            <strong><%= user["first_name"] %> <%= user["last_name"] %></strong>
            <p><%= user["email"]%></p>
            <p><%= user["id"]%></p>
          </div>
        </div>
        
          <nav class="level">
            <div class="level-left is-mobile">
                <%#= link_to "Join meeting", room, class: "level-item" %>
                <%#= link_to edit_room_path(room), class: "level-item" do %>
                  <!-- <span class="icon"><i class="fa fa-pencil" aria-hidden="true"></i></span> -->
                <%# end %>
                <%= link_to user_delete_path(user["id"]), method: :delete, data: { confirm: 'Are you sure want to delete Room?' } do%>
                  <span class="icon"><i class="fa fa-trash-o" aria-hidden="true"></i></span>
                <% end %>
              
            </div>
          </nav>
        
      </article>
    </div>
  <% end %>
</div>
    